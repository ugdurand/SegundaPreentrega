<h1>Vista Principal</h1>

<table>
    <thead>
        <tr>
            <th>Titulo</th>
            <th>Descripcion</th>
            <th>Codigo</th>
            <th>Precio</th>
        </tr>
        </tr>
    </thead>
    <tbody>
        {{#each products}}
        <tr>
                <td>{{this.title}}</td>
                <td>{{this.description}}</td>
                <td>{{this.code}}</td>
                <td>{{this.price}}</td>
            </tr>
        {{/each}}
    
    </tbody>
</table>